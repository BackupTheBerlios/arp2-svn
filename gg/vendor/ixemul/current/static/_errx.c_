#ifdef __PPC__

#include <err.h>
#include <stdarg.h>

__dead void
#if __STDC__
_errx(int eval, const char *fmt, ...)
#else
_errx(va_alist)
	va_dcl
#endif
{
	va_list ap;
#if __STDC__
	va_start(ap, fmt);
#else
	int eval;
	const char *fmt;

	va_start(ap);
	eval = va_arg(ap, int);
	fmt = va_arg(ap, const char *);
#endif
	_verrx(eval, fmt, ap);
	va_end(ap);
}

#else
#include <sys/cdefs.h>

#ifdef __indr_reference
__indr_reference(_errx, errx);
#endif
#endif
